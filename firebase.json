{
  "firestore": {
    "rules": ".firebase/firestore.rules",
    "indexes": ".firebase/firestore.indexes.json"
  },
  "hosting": [
    {
      "source": ".",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "frameworksBackend": {
        "region": "europe-west1"
      }
    },
    {
      "target": "try-angularfire",
      "source": ".",
      "frameworksBackend": {}
    }
  ]
}